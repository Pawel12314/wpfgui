<UserControl x:Class="Projekt_WPF.userControls.DiagramControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Projekt_WPF.userControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Name="diagram"
            xmlns:conv="clr-namespace:Projekt_WPF.conversion2"
             >
    <UserControl.Resources>
        <conv:ColumnDiagramConversion x:Key="lengthConv"></conv:ColumnDiagramConversion>
    </UserControl.Resources>
    <Grid>
        <ProgressBar Background="Yellow" Height="100" Width="{Binding Path =MyProperty, ElementName=diagram, Converter={StaticResource lengthConv }}" ></ProgressBar>
    </Grid>
</UserControl>
