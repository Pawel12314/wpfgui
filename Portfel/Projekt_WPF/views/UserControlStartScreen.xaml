<Page x:Class="Projekt_WPF.views.UserControlStartScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Projekt_WPF.views"
             mc:Ignorable="d" 
             xmlns:mycontrols="clr-namespace:Projekt_WPF.userControls"
             d:DesignHeight="450" d:DesignWidth="800"
      Loaded="Page_Loaded"
             
      >
    <Grid x:Name="mainGrid" Background="GhostWhite">
        <mycontrols:ButtonsListControl x:Name="menu" AddProperty="{Binding addCommand}"  MyWidthProperty="300" MyHeightProperty="100" OrientationProperty="Horizontal">
            
        </mycontrols:ButtonsListControl>
    </Grid>
</Page>
